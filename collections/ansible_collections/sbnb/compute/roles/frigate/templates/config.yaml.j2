# Frigate minimal config - generated by Ansible
# Replace the dummy camera with your actual cameras

auth:
  enabled: {{ sbnb_frigate_auth_enabled | lower }}

mqtt:
  enabled: false

cameras:
  dummy:
    enabled: false
    ffmpeg:
      inputs:
        - path: rtsp://localhost:554/dummy
          roles:
            - detect
    detect:
      width: 640
      height: 480
detect:
  enabled: true
